generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DaySummary {
  station   String
  date      DateTime @db.Date
  latitude  Float
  longitude Float
  elevation Float
  name      String
  temp      Float
  dewp      Float
  slp       Float
  stp       Float
  visib     Float
  wdsp      Float
  mxspd     Float
  gust      Float
  max       Float
  min       Float
  prcp      Float
  sndp      Float
  frshtt    String

  @@id([station, date])
  @@map("day_summary")
}
